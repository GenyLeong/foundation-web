RewriteEngine On

Redirect /directory.html		http://www.gnome.org/contact/
Redirect /news.html				http://foundation.gnome.org/news/

# Move of the documentation stuff
Redirect /documentation.html		http://foundation.gnome.org/documentation/
Redirect /charter.html			http://foundation.gnome.org/documentation/charter.html
Redirect /bylaws.pdf			http://foundation.gnome.org/documentation/bylaws.pdf

# Move of the membership stuff
Redirect /membership.html		http://foundation.gnome.org/membership/
Redirect /membership-list.html		http://foundation.gnome.org/membership/members.html
Redirect /membership-policy.html	http://foundation.gnome.org/membership/policy.html
Redirect /membership-form.html		http://foundation.gnome.org/membership/application.php
Redirect /membership-form.php		http://foundation.gnome.org/membership/application.php

# Move of the organization stuff
Redirect /organization.html		http://foundation.gnome.org/organization/
Redirect /advisory_board.html		http://foundation.gnome.org/organization/advisory_board.html
Redirect /board_of_directors-2000.html	http://foundation.gnome.org/organization/board_of_directors-2000.html
Redirect /board_of_directors-2001.html	http://foundation.gnome.org/organization/board_of_directors-2001.html
Redirect /board_of_directors.html	http://foundation.gnome.org/organization/board_of_directors.html
Redirect /executive_director.html	http://foundation.gnome.org/organization/executive_director.html

# Move of the elections stuff in subdirectories
